            $("#stockavailable").css('display', 'block');
        }
        else if (record.inventory < parseInt(document.getElementById('ctl00_ContentPlaceHolder1_Add_To_Cart_MakeMeAddtoCart_hfBulkQu').value)) {
            $('#instock').html("<div class='outofstock'>Out of stock </div>");
